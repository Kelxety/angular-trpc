{
  "name": "web-trpc",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/web/trpc/src",
  "prefix": "conduit",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/web/trpc/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/web/trpc/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/web/trpc/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/web/trpc/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/web/trpc/**/*.ts", "libs/web/trpc/**/*.html"]
      }
    }
  },
  "tags": []
}
